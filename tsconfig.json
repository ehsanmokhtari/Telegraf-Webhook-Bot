{
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"moduleResolution": "node",
		"module": "commonjs",
		"sourceMap": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"outDir": "dist",
		"lib": ["ESNext", "DOM"],
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules"]
}
