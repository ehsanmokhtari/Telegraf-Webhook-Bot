{
  "compilerOptions": {
    "strict": true,
    "target": "ES6",
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
        "src/**/*",
        "api/**/*",
    ],
  "exclude": ["node_modules"]
}
